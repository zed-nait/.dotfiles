[core]
  fileMode = false
  editor = nvim
  excludesfile = gitignore
[alias]
  c = commit
  ca = commit -a
  cm = commit -m
  cam = commit -am
  d = diff
  dc = diff --cached
  l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit
  la = log --graph --oneline --all
[init]
  defaultBranch = master
  fileMode = false
[pull]
  rebase = true
